  "tokenColors": [
    {
      "name": "plain",
      "scope": ["source", "support.type.property-name.css"],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "comment",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#a1a1a1"
      }
    },
    {
      "name": "constant",
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "entity",
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "variable",
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "function",
      "scope": ["entity.name.function", "support.type.primitive"],
      "settings": {
        "foreground": "#b675f1"
      }
    },
    {
      "name": "component",
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "operator",
      "scope": [
        "keyword.operator",
        "storage.type.function.arrow",
        "punctuation.separator.key-value.css"
      ],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "storage",
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "storageJava",
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "string",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#58c760"
      }
    },
    {
      "name": "support",
      "scope": ["support"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "supportProperty",
      "scope": [
        "support.type.object.module",
        "variable.other.object",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "propertyName",
      "scope": ["meta.property-name"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "variableOther",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "invalid",
      "scope": ["invalid.broken", "invalid.deprecated", "invalid.illegal", "invalid.unimplemented"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "messageError",
      "scope": ["message.error"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "stringVariable",
      "scope": ["string variable"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "regexp",
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "regexpCharacterClass",
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "regexpEscape",
      "scope": ["string.regexp constant.character.escape"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "supportConstant",
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "supportVariable",
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "metaModuleReference",
      "scope": ["meta.module-reference"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "markdownListMarker",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#f99902"
      }
    },
    {
      "name": "markdownHeading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "markdownQuote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "markdownItalic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ededed"
      }
    },
    {
      "name": "markdownBold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ededed"
      }
    },
    {
      "name": "markdownRaw",
      "scope": ["markup.raw"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "markdownDeleted",
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "markdownInserted",
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "markdownChanged",
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "foreground": "#f99902"
      }
    },
    {
      "name": "markdownIgnored",
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "name": "metaDiffRange",
      "scope": ["meta.diff.range"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b675f1"
      }
    },
    {
      "name": "metaDiffHeader",
      "scope": ["meta.diff.header"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "metaSeparator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "metaOutput",
      "scope": ["meta.output"],
      "settings": {
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "bracketHighlighter",
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#ededed"
      }
    },
    {
      "name": "bracketHighlighterUnmatched",
      "scope": ["brackethighlighter.unmatched"],
      "settings": {
        "foreground": "#f05b8d"
      }
    },
    {
      "name": "link",
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#62a6ff"
      }
    },
    {
      "name": "attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b675f1"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation",
        "punctuation.terminator",
        "punctuation.definition.tag",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#a1a1a1"
      }
    }
  ]
}
